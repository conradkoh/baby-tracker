{
  "name": "@workspace/mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:clean": "yarn cache clean && yarn install --network-timeout 100000 && yarn expo start --ios -c",
    "dev": "BRANCH=dev expo start",
    "android": "expo run:android",
    "ios": "yarn dev --ios",
    "web": "yarn dev --web",
    "build": "eas build --platform all",
    "build:ios": "eas build --platform ios",
    "build:local:ios": "expo run:ios",
    "build:preview:ios": "eas build --platform ios --profile preview",
    "build:development:ios": "eas build --platform ios --profile development",
    "build:development:ios-simulator": "eas build --profile development-simulator --platform ios",
    "build:production:ios": "eas build --platform ios --profile production",
    "update:preview": "eas update --branch preview",
    "update:production": "eas update --branch production",
    "device:add": "eas device:create",
    "submit:ios": "eas submit -p ios",
    "doc": "npx expo-doctor",
    "sysreset": "yarn cache clean && lsof -t -i :8081 | xargs kill"
  },
  "workspaces": {
    "nohoist": [
      "**"
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/datetimepicker": "7.6.1",
    "@react-native-picker/picker": "2.6.1",
    "@shopify/flash-list": "1.6.3",
    "@workspace/backend": "*",
    "@workspace/domain": "*",
    "convex": "^1.9.1",
    "expo": "~50.0.6",
    "expo-clipboard": "~5.0.1",
    "expo-constants": "~15.4.5",
    "expo-crypto": "~12.8.1",
    "expo-device": "~5.9.3",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.7",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "~0.24.10",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-vector-icons": "^10.0.3",
    "react-use": "^17.5.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/luxon": "^3.4.2",
    "@types/react": "~18.2.14",
    "@types/react-native-vector-icons": "^6.4.18",
    "jest": "^29.7.0",
    "tailwindcss": "3.3.2",
    "typescript": "^5.3.3"
  },
  "private": true
}
